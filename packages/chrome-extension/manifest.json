{
    "name": "ho-nft-extension",
    "description": "My humble opionion of this NFT ...",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_icon": "icon-34.png",
        "default_title": "HO NFT"
    },
    "icons": {
        "34": "icon-34.png",
        "48": "icon-48.png",
        "128": "icon-128.png",
        "256": "icon-256.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*.opensea.io/*"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "antd.min.css"
            ]
        }
    ],
    "web_accessible_resources": [
        {
          "resources": [
                "antd.min.css",
                "icon-34.png",
                "icon-48.png",
                "icon-128.png",
                "icon-256.png"
            ],
          "matches": []
        }
    ]
}